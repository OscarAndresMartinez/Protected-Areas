foreach var1 of varlist GDPpercapitacurrentUS Percentage_Tree_cover_loss income_group Continents cce rle rqe gee Arablelandoflandarea precipitationmm MeantemperaturyearC Populationdensity Agricultureforestryandfishi {
	display "`var1'"
	
did_multiplegt RLI ID year percentacumestricta, robust_dynamic dynamic(12) threshold_stable_treatment(1) reca(D_cat) breps(100) average_effect placebo(6) longdiff_placebo jointtestplacebo controls(`var') cluster(ID) graphoptions(title(Effect strictly Protected area on RLI)) 
	
}

foreach var of varlist GDPpercapitacurrentUS Percentage_Tree_cover_loss income_group Continents cce rle rqe gee Arablelandoflandarea precipitationmm MeantemperaturyearC Populationdensity Agricultureforestryandfishi {

	foreach var2 of varlist GDPpercapitacurrentUS Percentage_Tree_cover_loss income_group Continents cce rle rqe gee Arablelandoflandarea precipitationmm MeantemperaturyearC Populationdensity Agricultureforestryandfishi {
	
	foreach var3 of varlist GDPpercapitacurrentUS Percentage_Tree_cover_loss income_group Continents cce rle rqe gee Arablelandoflandarea precipitationmm MeantemperaturyearC Populationdensity Agricultureforestryandfishi {
	

display "`var1'" " "  "`var2'"  " "  "`var3'"

did_multiplegt RLI ID year percentacumestricta, robust_dynamic dynamic(12) threshold_stable_treatment(1) reca(D_cat) breps(100) average_effect placebo(6) longdiff_placebo jointtestplacebo controls(`var1' `var2' `var3') cluster(ID) graphoptions(title(Effect strictly Protected area on RLI)) //ok 
	}
	}
}

