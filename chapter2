clear all

use "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VII\base de datos ap pura.dta"

	drop if year==.
	drop if metadataid==.
	drop if type == "Point"
	drop if iucn_cat == "Not Reported"
	drop if iucn_cat == "Not Applicable"
	drop if status == "Adopted"
	drop if status == "Established"
	drop if status == "Inscribed"
	drop if status == "Proposed"
	drop if desig_eng == "Ramsar Site, Wetland of International Importance" 
	drop if desig_eng == "UNESCO-MAB Biosphere Reserve" 
	drop if desig_eng == "World Heritage Site"
	drop if desig_type == "International"
	drop if country == "MNP;GUM"
	drop if country == "MNP;UMI"
	drop if country == "BFA;NER;BEN"
	///// drop marine
	drop if marine >0
	drop if desig_eng =="National Marine Conservation Area"
	drop if desig_eng =="Marine Protected Area"
	drop if desig_eng =="Marine Reserve"
	drop if desig_eng =="Marine Preserve"
	drop if desig_eng =="Marine Park Addition"
	drop if desig_eng =="Marine Management Area"
	drop if desig_eng =="Marine National Park"
	drop if desig_eng =="Coastline Marine Resource Development Area, designated sea area"
	drop if desig_eng =="Marine National Reserve"
	drop if desig_eng =="National Marine Park"
	drop if desig_eng =="Marine Park"
	drop if desig_eng =="Marine Nature Recreation Park"
	drop if desig_eng =="State Marine Park"
	drop if desig_eng =="Nature Reserve and Marine Reserve"
	drop if desig_eng =="Locally Managed Marine Area"
	drop if desig_eng =="Marine Managed Area"
	drop if desig_eng =="Natural Marine Reserve and Natural Protected Marine Areas"
	drop if desig_eng =="Wildlife Reserve / Marine Park"
	drop if desig_eng =="Marine Conservation Area"
	drop if desig_eng =="Marine Conservation Area / Wildlife Reserve"
	drop if desig_eng =="Marine Reserve / Marine Conservation Area / Wildlife Reserve"
	drop if desig_eng =="Marine Park / Marine Reserve / Wildlife Reserve"
	drop if desig_eng =="Marine Nature Reserve"
	drop if desig_eng =="Special Marine Reserve"
	drop if desig_eng =="Marine Biological Reserve"
	drop if desig_eng =="Marine Protected Area (OSPAR)"
	drop if desig_eng =="Nature reserve zone in National Marine Park"
	drop if desig_eng =="Marine  Park"
	drop if desig_eng =="National Park and Marine Management Area"
	drop if desig_eng =="Marine National Park and ASEAN Heritage Park"
	drop if desig_eng =="Marine  Park (AMP)"
	drop if desig_eng =="Marine Park (AMP)"
	drop if desig_eng =="Marine Management Area / Marine Reserve"
	drop if desig_eng =="Local Fisheries Marine Management Area"
	drop if desig_eng =="International significance Natural Marine Area"
