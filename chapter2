	drop if desig_eng =="Marine Mammal Sanctuary"
	drop if desig_eng =="Marine Recreation Park"
	drop if desig_eng =="District Marine Protected Areas"
	drop if desig_eng =="Australian Marine Park"
	drop if desig_eng =="Marine Nature Park"
	drop if desig_eng =="Protected Marine Area"
	drop if desig_eng =="Turneffe Atoll Marine Reserve"
	drop if desig_eng =="Nature Conservation Marine Protected Area"
	drop if desig_eng =="Marine Protected Area (MPA)"
	drop if desig_eng =="Marine Fisheries Reserved Area"
	drop if desig_eng =="Marine Conservation Zone"
	drop if desig_eng =="Marine Park Buffer Zone"
	drop if desig_eng =="Marine Strict Nature Reserve"
	drop if desig_eng =="Marine National Wildlife Area"
	drop if desig_eng =="Marine Protected Area / Protected Land and Seascape"
	drop if desig_eng =="Marine Resource Reserve"
	drop if desig_eng =="Marine and Coastal Resources Protected Area"

	**# generate terrestrial area 

	generate repterr = gis_area - gis_m_area
	drop if repterr<0
	
	
**# generate groups by iucn category 

generate estrictiucn  = iucn_cat if inlist(iucn_cat, "Ia","Ib", "II")
gen estricta= repterr if estrictiucn!=""
gen noestricta= repterr if estrictiucn==""


**# collapse by year country 

collapse (sum) estricta (sum) noestricta, by(year country)
sort country year

**# merge extention by country database 

merge 1:1 year country using "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VI\extention_pais_forest_km2.dta"
sort country year


**# adjust data and generate: acumulate estricta and no estricta, and percentage each one  
 
replace estricta =0 if estricta ==.
replace noestricta =0 if noestricta ==.

bysort country: gen acumestricta=sum( estricta)
bysort country: gen acumnoestricta=sum( noestricta)

bysort country: gen percentacumestricta= ((acumestricta/extent_pais_km2)*100)
bysort country: gen percentacumnoestricta=((acumnoestricta/extent_pais_km2)*100)

drop _merge 

//generate total percent acumulate protected areas by country 

generate Total_PA = acumestricta + acumnoestricta
bysort country: gen Total_percent_PA =((Total_PA/extent_pais_km2)*100)

generate total_percent_PA = percentacumestricta + percentacumnoestricta



///generate mean by percentacumestricta and percentacumnoestricta
egen meanpercentacumestricta= mean(percentacumestricta), by (country)
egen meanpercentacumnoestricta= mean(percentacumnoestricta), by (country)
egen meantotal_PA=mean(Total_percent_PA), by(country)



**# merge BII and RLI index database and rename label each index 
merge 1:1 year country using "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VI\BII_ and_ RLI.dta"

sort country year
drop _merge 

generate RLI= rli*100
generate BII= BII_index*100
drop rli
drop BII_index
label variable BII "Biodiversity Intactness Index"
label variable RLI "Red List Index "

**# merge SHI index database and rename label 
///mol data 
merge 1:1 year country using "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VII\shi_country_year_agg_MOL.dta" //

sort country year
drop _merge 
rename national_shi SHI 
label variable SHI "Species Habitat Index"


**# merge covariables 

merge 1:1 year country using "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VI\covariates_full_new_2.dta"
sort country year


**# keep year from 2001 to 2014
keep if _merge==3 
keep if year>=2001 & year<=2014
drop _merge


***********************************************************
**# group income countries 

merge 1:1 year country using "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VIII\Income groups by country.dta"
sort country year
keep if _merge==3 
drop _merge


encode incomegroup, generate(income_group)
label define income_grouplbl 1 "High income (H)" 2 "Low income (L)" 3 "Lower middle income (LM)"  4 "No data (ND)" 5 "Upper middle income (UM)"
label values income_group income_grouplbl 


gen High_income=1 if incomegroup=="H"
replace High_income=0 if High_income==.

gen Low_income=1 if incomegroup=="L"
replace Low_income=0 if Low_income==.  

gen Lower_middle_income =1 if incomegroup=="LM"
replace Lower_middle_income=0 if Lower_middle_income==.

gen Upper_middle_income=1 if incomegroup=="UM"
replace Upper_middle_income=0 if Upper_middle_income==.

gen No_data=1 if incomegroup=="ND"  
replace No_data=0 if No_data==. 




///merge tree cover loss 
merge 1:1 year country using "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VIII\Treecover_loss_by_region__ha.dta"
sort country year
keep if _merge==3 
drop _merge


///adjust variable tree_cover_loss percentage   

bysort country: generate Tree_cover_loss__km2 = (Tree_cover_loss__ha/100)
bysort country: generate Percentage_Tree_cover_loss= (Tree_cover_loss__km2/extent_pais_km2)*100
label variable Percentage_Tree_cover_loss "Percentage Tree Cover Loss"


/////GDP_percapita 

merge 1:1 year country using "C:\Users\oscarandresmartinez\OneDrive - Universidad de los Andes\PhD\VIII\gdp_percapita.dta"
sort country year
keep if _merge==3 
drop _merge




//drop merge and points by index and null data by country 
drop _merge 

drop if RLI==.
drop if SHI==. 
drop if BII==. 
drop if country == "SRB" //ok
drop if country == "SDN" //ok
drop if country=="JOR" //ok by Percentage_Tree_cover_loss
drop if country=="MLT"
drop if country=="MRT"
drop if country=="NER"

drop if country=="VUT" //ok rqe
drop if country=="CPV"



**# determinate panel data 

xtset ID year, yearly


**# generate global covariables

global xvar "GDPgrowthannual changetempyear changepopulationdensityyea changeprecip Arablelandoflandarea percentforest_country percentagric_country gee rqe rle cce"

global xvarb "GDPgrowthannual Agricultureforestryandfishi Agriculturearea1000ha MeantemperaturyearC Populationdensity precipitationmm Arablelandoflandarea percentforest_country percentagric_country gee rqe rle cce America Europe Africa Asia Oceania Tree_cover_loss__ha High_income Low_income Lower_middle_income Upper_middle_income"



***#///tabstat variables by country and continent 

tabstat RLI BII SHI Total_percent_PA percentacumestricta percentacumnoestricta, statistics( mean max min ) by(Countryname) noseparator labelwidth(100) varwidth(100) columns(statistics) longstub format(%4.3f)

tabstat RLI BII SHI Total_percent_PA percentacumestricta percentacumnoestricta, statistics( mean max min ) by(Continents) noseparator labelwidth(100) varwidth(100) columns(statistics) longstub format(%4.3f)

sum GDPgrowthannual Agricultureforestryandfishi Agriculturearea1000ha Populationdensity precipitationmm Arablelandoflandarea percentforest_country percentagric_country gee rqe rle cce,d

**# CORRELATION 
///Protected areas
correlate percentacumestricta percentacumnoestricta Total_percent_PA, covariance
pwcorr percentacumestricta percentacumnoestricta Total_percent_PA, sig print(.01) star(.01)
spearman percentacumestricta percentacumnoestricta Total_percent_PA, star(.05) stats(rho obs p) bonferroni
correlate RLI BII SHI, covariance
pwcorr RLI BII SHI, sig print(.01) star(.01)
spearman RLI BII SHI, star(.05) stats(rho obs p) bonferroni
pwcorr percentacumestricta ID, sig print(.01) star(.01) 
			 
			 
			 
			 
correlate percentacumestricta percentacumnoestricta RLI BII SHI GDPpercapitacurrentUS Percentage_Tree_cover_loss cce rle rqe gee Arablelandoflandarea precipitationmm Populationdensity MeantemperaturyearC Agricultureforestryandfishi, covariance


pwcorr year percentacumestricta percentacumnoestricta total_percent_PA BII RLI SHI GDPgrowthannual Agricultureforestryandfishi Agriculturearea1000ha MeantemperaturyearC Populationdensity precipitationmm Arablelandoflandarea percentforest_country percentagric_country gee rqe rle cce America Europe Africa Asia Oceania Tree_cover_loss__ha High_income Low_income Lower_middle_income Upper_middle_income

pwcorr year percentacumestricta percentacumnoestricta total_percent_PA BII RLI SHI GDPgrowthannual Agricultureforestryandfishi Agriculturearea1000ha MeantemperaturyearC Populationdensity precipitationmm Arablelandoflandarea percentforest_country percentagric_country gee rqe rle cce America Europe Africa Asia Oceania Tree_cover_loss__ha High_income Low_income Lower_middle_income Upper_middle_income, sig

pwcorr year percentacumestricta percentacumnoestricta total_percent_PA BII RLI SHI GDPgrowthannual Agricultureforestryandfishi Agriculturearea1000ha MeantemperaturyearC Populationdensity precipitationmm Arablelandoflandarea percentforest_country percentagric_country gee rqe rle cce America Europe Africa Asia Oceania Tree_cover_loss__ha High_income Low_income Lower_middle_income Upper_middle_income, sig print(.01) star(.01)


pwcorr year percentacumestricta percentacumnoestricta total_percent_PA BII RLI SHI GDPpercapitagrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Populationdensity Arablelandoflandarea percentforest_country Percentage_Tree_cover_loss income_group, sig print(.01) star(.01)

spearman year percentacumestricta percentacumnoestricta total_percent_PA BII RLI SHI GDPpercapitagrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Populationdensity Arablelandoflandarea percentforest_country Percentage_Tree_cover_loss income_group, star(.05) stats(rho obs p) bonferroni


///statistic covariables FIXED model

xtreg RLI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss, fe 

xtreg BII GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss, fe

xtreg SHI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss, fe



// random effect model
xtreg RLI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss, re
xtreg BII  GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss, re
xtreg SHI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss, re
xtcsd, pesaran abs


//Braush-Pagan (LM) test: OLS vs RE 
quietly xtreg RLI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce  Percentage_Tree_cover_loss, re 
quietly xtreg BII GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce  Percentage_Tree_cover_loss, re 
quietly xtreg SHI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce  Percentage_Tree_cover_loss, re
xttest0

//Hausman test 
 quietly xtreg RLI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss,fe 
estimate store fe 
quietly xtreg RLI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss,re 
estimate store re 
hausman fe re
quietly xtreg BII GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss,fe 
estimate store fe 
quietly xtreg BII GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss,re 
estimate store re 
hausman fe re
quietly xtreg SHI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss,fe 
estimate store fe 
quietly xtreg SHI GDPgrowthannual Agricultureforestryandfishi MeantemperaturyearC precipitationmm Arablelandoflandarea percentforest_country gee rqe rle cce Percentage_Tree_cover_loss,re 
estimate store re 
hausman fe re



////generate continents variable 
//America 
generate Continents = 1 if inlist(country, "ATG", "BHS", "BRB", "BLZ", "CAN")| inlist(country, "CRI", "CUB", "DMA", "DOM", "SLV") | inlist(country,"GRD", "GTM","HTI", "HND", "JAM")|inlist(country, "NIC","MEX", "PAN","PRI","TTO") |inlist(country,"USA", "ARG","BOL", "BRA")| inlist(country,"CHL","COL", "ECU", "GUY") |inlist(country,"PRY","PER","SUR","URY","VEN") 
//Asia
replace Continents = 2 if inlist(country,"CHN", "QAT", "SAU", "PHL", "TWN")| inlist(country,"MYS", "ARE", "LBN", "JOR", "UZB") |inlist(country,"KGZ", "AZE", "TKM","TLS", "BTN", "LAO") | inlist(country,"KWT", "KHM", "IRQ", "AFG", "KAZ","BGD") | inlist(country,"MDV", "KOR", "VNM", "IND", "JPN") | inlist(country,"SGP", "THA", "PRK", "SYR" "NPL")| inlist(country,"MMR", "ARM", "BHR", "PSE", "BRN") | inlist(country,"CCK", "TJK", "MAC", "MNG", "OMN") |inlist(country,"YEM", "LKA", "HKG", "IDN", "ISR", "PAK", "NPL", "IRN")
///Africa
replace Continents = 3 if inlist(country,"MAR", "MUS", "DZA", "TUN", "NGA") | inlist(country,"SEN", "ZAF", "MDG", "ETH", "CPV") | inlist(country,"COD", "GHA", "SYC", "KEN", "TZA") | inlist(country,"MLI", "SDN", "CMR", "LBY", "NER") | inlist(country,"UGA", "SOM", "RWA", "CIV", "AGO") | inlist(country,"ZWE", "BFA", "ERI", "TCD", "MRT") | inlist(country,"NAM", "MOZ", "ZMB", "LBR", "SLE") | inlist(country,"BEN", "SWZ", "SSD", "GIN", "COG", "BDI") | inlist(country,"BWA", "GMB", "TGO", "GAB", "LSO") | inlist(country,"DJI", "GNQ", "MWI","CAF","COM", "GNB", "STP", "EGY")
///Europe
replace Continents = 4 if inlist(country,"ALB", "DEU", "ARM", "AUT", "BEL") | inlist(country,"BLR", "BIH", "BGR", "CYP", "HRV" ) | inlist(country,"DNK","SVK", "SVN", "ESP", "EST") |inlist(country,"RUS", "FIN", "FRA", "GEO","GIB") | inlist(country,"GRC", "HUN", "IRL", "ISL")| inlist(country,"ITA", "LIE", "LTO", "LUX", "MKD")| inlist(country,"MLT", "MDA", "NOR","NLD", "POL") |inlist(country,"GBR", "CZE", "ROU", "CHE", "SWE") | inlist(country,"TUR", "UKR", "PRT", "LTU", "GRL" )
//Oceania
replace Continents = 5 if inlist(country,"AUS", "NZL", "FJI", "GUM", "WSM")| inlist(country,"PNG", "NCL", "VUT", "PLW", "TUV") |inlist(country,"KIR", "TON", "NRU", "PYF", "COK") | inlist(country,"SLB", "MHL" "ASM", "NFK", "NIU") | inlist(country,"FSM", "PCN", "MNP", "WLF")
tab Continents

label define contlbl 1 "America" 2 "Asia" 3 "Africa"  4 "Europe" 5 "Oceania"
label values Continents contlbl
reg RLI  i.Continents percentacumestricta 
reg RLI  i.Continents percentacumestricta Continents#c.percentacumestricta

////difference km2 acumestricta by year 2000 and 2014

gen htacumestricta= acumestricta if year==2000
egen meanacumestricta = mean(htacumestricta), by (Countryname)

gen gtacumestricta= acumestricta if year==2014
egen mean_acumestricta = mean(gtacumestricta), by (Countryname)

gen difmean = mean_acumestricta - meanacumestricta 

///extention estricta by year 2007 and generate difference 2007-2000

gen extacumestricta2007 = acumestricta if year==2007
egen meanacumestricta2007 =mean(extacumestricta2007), by(Countryname)

gen dif2007_2000 = meanacumestricta2007 - meanacumestricta 
tab dif2007_2000  

///generate variable difmean in percentage 

gen pdifmean = (difmean/extent_pais_km2)*100

///generate variable dif2007_2000  in percentage estricta 

gen pdif2007_2000 = (dif2007_2000 /extent_pais_km2)*100


/// generate extention acumnoestricta by year 2007 y 2000, generate difference between years  

generate noestricta_acum2007 = acumnoestricta if year==2007 
generate noestricta_acum2000 = acumnoestricta if year==2000

egen meannoestricta_acum2007 = mean(noestricta_acum2007), by (Countryname)
egen meannoestricta_acum2000 = mean(noestricta_acum2000), by (Countryname)

generate diff_noestricta_2007_2000 = meannoestricta_acum2007 - meannoestricta_acum2000

generate per_diff_noestricta_2007_2000 = (diff_noestricta_2007_2000/ extent_pais_km2)*100

tab per_diff_noestricta_2007_2000 
label variable per_diff_noestricta_2007_2000 "Cambio porcentual en superficie de AP no estrictas entre 2000-2007"

/// generate difference km2 acumnoestrictas by year 2000 and 2014

gen acumnoestricta2000= acumnoestricta if year==2000
egen meanacumnoestricta2000 = mean(acumnoestricta2000), by (Countryname)

gen acumnoestricta2014= acumnoestricta if year==2014
egen meanacumnoestricta2014 = mean(acumnoestricta2014), by (Countryname)

gen difmeannoestrict = meanacumnoestricta2014 - meanacumnoestricta2000 
gen pdifmeannoestrict = (difmeannoestrict/extent_pais_km2)*100



**#generate difference index by year 2001-2014

************************
///2001
gen RLI2001 = RLI if year==2001
egen meanRLI2001 = mean(RLI2001), by (Countryname)

gen BII2001 = BII if year==2001
egen meanBII2001 = mean(BII2001), by (Countryname)

gen SHI2001 = SHI if year==2001
egen meanSHI2001 = mean(SHI2001), by (Countryname)


**************************
////2014

gen RLI2014 = RLI if year==2014
egen meanRLI2014 = mean(RLI2014), by (Countryname)

gen BII2014 = BII if year==2014
egen meanBII2014 = mean(BII2014), by (Countryname)

gen SHI2014 = SHI if year==2014
egen meanSHI2014 = mean(SHI2014), by (Countryname)


**********

gen difRLI_ = meanRLI2014 - meanRLI2001
gen difBII_= meanBII2014- meanBII2001
gen difSHI_= meanSHI2014- meanSHI2001


**# generate difference PAs by year 2001-2014

///2001

gen Total_percent_PA2001 = Total_percent_PA if year==2001
egen meanTotal_percent_PA2001= mean(Total_percent_PA2001), by (Countryname)

gen percentacumestricta2001 = percentacumestricta if year==2001
egen meanpercentacumestricta2001= mean(percentacumestricta2001), by (Countryname)

gen percentacumnoestricta2001 = percentacumnoestricta if year==2001
egen meanpercentacumnoestricta2001= mean(percentacumnoestricta2001), by (Countryname)

///2014

gen Total_percent_PA2014 = Total_percent_PA if year==2014
egen meanTotal_percent_PA2014= mean(Total_percent_PA2014), by (Countryname)

gen percentacumestricta2014 = percentacumestricta if year==2014
egen meanpercentacumestricta2014= mean(percentacumestricta2014), by (Countryname)

gen percentacumnoestricta2014 = percentacumnoestricta if year==2014
egen meanpercentacumnoestricta2014= mean(percentacumnoestricta2014), by (Countryname)


gen Total_percent_PA_diff = meanTotal_percent_PA2001 - meanTotal_percent_PA2001
gen percentacumestricta_diff= meanpercentacumestricta2014 - meanpercentacumestricta2001
gen percentacumnoestricta_diff= meanpercentacumnoestricta2014- meanpercentacumnoestricta2001

label variable difRLI_ "Difference RLI 2001-2014"

label variable difBII_ "Difference BII 2001-2014"

label variable difSHI_ "Difference SHI 2001-2014"




**# Graphics

///index - percentacumestricta percentacumnoestricta and total_ap  difference 2014-2001
twoway (scatter difRLI percentacumestricta) (scatter difBII percentacumestricta) (scatter difSHI percentacumestricta), yline(0, lcolor(cranberry)) ytitle(`"Percentage index"') xtitle(`"Cumulative percentage Strictly PAs"') legend(off) 
twoway (scatter difRLI percentacumnoestricta) (scatter difBII percentacumnoestricta) (scatter difSHI percentacumnoestricta), yline(0, lcolor(cranberry)) ytitle(`"Percentage index"') xtitle(`"Cumulative percentage Not Strictly PAs "') legend(off) 
twoway (scatter difRLI total_percent_PA) (scatter difBII total_percent_PA) (scatter difSHI total_percent_PA), yline(0, lcolor(cranberry))  ytitle(`"Percentage index"') xtitle(`"Cumulative percentage PAs"') legend(position(6) span col(3)) 
twoway (scatter difRLI total_percent_PA) (scatter difBII total_percent_PA) (scatter difSHI total_percent_PA), yline(0, lcolor(cranberry))  ytitle(`"Percentage"') xtitle(`"Cumulative percentage PAs"') by(Continents)  legend(position(6) span col(3)) 




///index - mean percentacumestricta mean percentacumnoestricta difference 2014-2001

twoway (scatter difRLI Total_percent_PA_diff ) (scatter difBII Total_percent_PA_diff ) (scatter difSHI Total_percent_PA_diff ), yline(0, lcolor(cranberry))
twoway (scatter difRLI percentacumestricta_diff) (scatter difBII percentacumestricta_diff) (scatter difSHI percentacumestricta_diff), yline(0, lcolor(cranberry))
twoway (scatter difRLI percentacumnoestricta_diff) (scatter difBII percentacumnoestricta_diff) (scatter difSHI percentacumnoestricta_diff), yline(0, lcolor(cranberry))


///2001
